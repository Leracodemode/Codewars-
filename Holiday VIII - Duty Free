function dutyFree(normPrice, discount, hol){
  let numberOfBottles = 1;
  let discountRate = normPrice * discount / 100;
  let discountPrice = normPrice - discountRate;
  let saving = normPrice - discountPrice;
    numberOfBottles = Math.floor(hol / saving);
  return numberOfBottles;
}
